node n1 {
    type router
    model host
    network-config {
	hostname n1
	!
	interface eth2
	 ip address 10.0.2.10/24
	 ipv6 address 2001:2::10/64
	!
	interface eth1
	 ip address 10.0.1.10/24
	 ipv6 address 2001:1::10/64
	!
	interface eth0
	 ip address 10.0.0.11/24
	 ipv6 address 2001:0::11/64
	!
    }
    canvas c1
    iconcoords {194.0 333.0}
    labelcoords {194.0 365.0}
    interface-peer {eth0 n3}
    interface-peer {eth1 n4}
    interface-peer {eth2 n2}
    custom-config {
	custom-config-id service:UserDefined
	custom-command UserDefined
	config {
	files=('mtu_client.py', 'server.c', 'mtu-hmi_server.c', )
	}
    }
    custom-config {
	custom-config-id service:UserDefined:mtu-hmi_server.c
	custom-command mtu-hmi_server.c
	config {
	file:///home/vakhil/Desktop/pampana/Extra-project/mtu-hmi_server.c
	}
    }
    custom-config {
	custom-config-id service:UserDefined:server.c
	custom-command server.c
	config {
	file:///home/vakhil/Desktop/pampana/Extra-project/mtu-hmi_server.c
	}
    }
    custom-config {
	custom-config-id service:UserDefined:mtu_client.py
	custom-command mtu_client.py
	config {
	file:///home/vakhil/Desktop/pampana/Extra-project/server.c
	}
    }
    services {DefaultRoute SSH UserDefined}
}

